{
  "name": "azuredatastudio-dmpwrapper",
  "version": "0.4.0",
  "description": "A wrapper to simplify using Data Management Protocol APIs in Azure Data Studio",
  "main": "dist/index.js",
  "typings": "dist/index",
  "engines": {
    "vscode": "^1.28.0",
    "sqlops": "*"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "prepare": "npm run compile",
    "watch": "tsc -watch -p ./",
    "updatesqlops": "curl -o ./typings/sqlops.proposed.d.ts https://raw.githubusercontent.com/Microsoft/azuredatastudio/master/src/sql/sqlops.proposed.d.ts",
    "installtypings": "node ./node_modules/vscode/bin/install && node ./node_modules/sqlops/bin/install"
  },
  "keywords": [
    "AzureDataStudio"
  ],
  "author": "kevcunnane",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.0.0",
    "braces": "^3.0.2",
    "sqlops": "github:anthonydresser/sqlops-extension-sqlops",
    "tar": "^4.4.8",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "vscode": "^1.1.34"
  },
  "dependencies": {}
}
