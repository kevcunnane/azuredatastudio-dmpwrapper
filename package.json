{
  "name": "azuredatastudio-dmpwrapper",
  "version": "1.0.0",
  "description": "A wrapper to simplify using Data Management Protocol APIs in Azure Data Studio",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevcunnane/azuredatastudio-dmpwrapper"
  },
  "main": "dist/index.js",
  "typings": "dist/index",
  "engines": {
    "vscode": "^1.28.0",
    "azdata": "*"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "prepare": "npm run compile",
    "watch": "tsc -watch -p ./",
    "updateazdata": "curl -o ./typings/azdata.proposed.d.ts https://raw.githubusercontent.com/Microsoft/azuredatastudio/master/src/sql/azdata.proposed.d.ts",
    "postinstall": "node ./node_modules/vscode/bin/install && node ./node_modules/azdata/bin/install"
  },
  "keywords": [
    "AzureDataStudio"
  ],
  "author": "kevcunnane",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.0.0",
    "azdata": "^1.0.0",
    "braces": "^3.0.2",
    "tar": "^4.4.8",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "vscode": "^1.1.34"
  },
  "dependencies": {}
}
